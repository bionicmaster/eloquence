{
    "name": "bionicmaster/eloquence",
    "description": "Flexible Searchable, Mappable, Metable, Validation and more extensions for Laravel Eloquent ORM.",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/jarektkaczyk/eloquence/issues",
        "source": "https://github.com/jarektkaczyk/eloquence"
    },
    "keywords": [
        "laravel",
        "eloquent",
        "metable",
        "searchable",
        "mappable",
        "mutable"
    ],
    "authors": [
        {
            "name": "Jarek Tkaczyk",
            "email": "jarek@softonsofa.com",
            "homepage": "http://softonsofa.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "bionicmaster/eloquence-base": "5.6.*",
        "bionicmaster/eloquence-metable": "5.6.*",
        "bionicmaster/eloquence-mutable": "5.6.*",
        "bionicmaster/eloquence-mappable": "5.6.*",
        "bionicmaster/eloquence-validable": "5.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "~1.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Sofa\\Eloquence\\Tests\\": "tests"
        }
    },
    "minimum-stability": "stable",
    "scripts": {
        "test": "phpunit"
    }
}
